{
  "name": "@k03mad/request",
  "version": "5.0.0",
  "description": "Request library",
  "maintainers": [
    "Kirill Molchanov <k03.mad@gmail.com"
  ],
  "main": "app/index.js",
  "repository": "k03mad/request",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "debug": "4.3.4",
    "got": "14.0.0",
    "p-queue": "8.0.1",
    "pretty-bytes": "6.1.1"
  },
  "devDependencies": {
    "@k03mad/eslint-config": "17.11.0",
    "eslint": "8.56.0",
    "husky": "8.0.3"
  },
  "scripts": {
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint ./ --cache",
    "clean": "npm run clean:modules && npm run clean:eslint:cache",
    "clean:modules": "rm -rf ./node_modules || true",
    "clean:eslint:cache": "rm -rf .eslintcache || true",
    "setup": "npm run clean && npm i",
    "setup:prod": "npm run clean && npm i --omit=dev",
    "prepare": "husky install || true"
  }
}
