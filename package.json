{
  "name": "@k03mad/request",
  "version": "5.6.1",
  "description": "Request library",
  "maintainers": [
    "Kirill Molchanov <k03.mad@gmail.com"
  ],
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k03mad/request.git"
  },
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "chalk": "5.3.0",
    "debug": "4.3.4",
    "got": "14.2.0",
    "p-queue": "8.0.1",
    "pretty-bytes": "6.1.1",
    "strip-ansi": "7.1.0"
  },
  "devDependencies": {
    "@k03mad/eslint-config": "20.3.0",
    "eslint": "8.56.0",
    "husky": "9.0.11"
  },
  "scripts": {
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint ./ --cache",
    "clean": "npm run clean:modules && npm run clean:eslint:cache",
    "clean:modules": "rm -rf ./node_modules || true",
    "clean:eslint:cache": "rm -rf .eslintcache || true",
    "setup": "npm run clean && npm run setup:pnpm",
    "setup:pnpm": "npm i pnpm -g && pnpm i",
    "prepare": "husky || true"
  }
}
